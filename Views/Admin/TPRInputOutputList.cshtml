
@{
    ViewBag.Title = "TPR Input Output List";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
    int co = 0;
}

<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">TPR Input Output List</h4>
                    <div><a href="/Admin/TPRInputOutput" style="text-decoration:underline !important">Go Back</a></div>
                    <br />
                    <h6 class="card-subtitle">Export data to Copy, CSV, Excel, PDF & Print</h6>
                    <div class="table-responsive m-t-30">
                        <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>Id</th>
                                    <th>Date</th>
                                    <th>Room No.</th>
                                    <th>Name Of Doctor</th>
                                    <th>Name Of Patient</th>
                                    <th>Shift Chart</th>
                                    <th>Time</th>
                                    <th>Temp</th>
                                    <th>Pulse</th>
                                    <th>Blood Pressure</th>
                                    <th>SPO2</th>
                                    <th>RBS</th>
                                    <th>Input Oral</th>
                                    <th>Input iv</th>
                                    <th>Output Urine</th>
                                    <th>Output Aspiration</th>
                                </tr>
                            </thead>
                            <tfoot>
                                <tr>
                                    <th>Id</th>
                                    <th>Date</th>
                                    <th>Room No.</th>
                                    <th>Name Of Doctor</th>
                                    <th>Name Of Patient</th>
                                    <th>Shift Chart</th>
                                    <th>Time</th>
                                    <th>Temp</th>
                                    <th>Pulse</th>
                                    <th>Blood Pressure</th>
                                    <th>SPO2</th>
                                    <th>RBS</th>
                                    <th>Input Oral</th>
                                    <th>Input iv</th>
                                    <th>Output Urine</th>
                                    <th>Output Aspiration</th>
                                </tr>
                            </tfoot>
                            <tbody>
                                @if (ViewBag.tprlist != null)
                                {
                                    foreach (SanskarHospital.Models.TPRInputOutputModel i in ViewBag.tprlist)
                                    {
                                <tr id="Busval_@i.patient_id">

                                    <td>@(++co)</td>
                                    <td>@i.date</td>
                                    <td>@i.room_no</td>
                                    <td>@i.doctor_id</td>
                                    <td>@i.patient_id</td>
                                    <td>@i.shift_chart</td>
                                    <td>@i.time</td>
                                    <td>@i.temp</td>
                                    <td>@i.pulse</td>
                                    <td>@i.blood_pressure</td>
                                    <td>@i.spo2</td>
                                    <td>@i.rbs</td>
                                    <td>@i.input_oral</td>
                                    <td>@i.input_iv</td>
                                    <td>@i.output_urine</td>
                                    <td>@i.output_aspiration</td>

                                </tr>
                                    }
                                }

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

